{
	"name": "blog-eleventy",
	"version": "1.0.0",
	"description": "A modified blog, based on the starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/blog-eleventy/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/blog-eleventy/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/aenfield/blog-eleventy.git"
	},
	"author": {
		"name": "Andrew Enfield",
		"email": "aenfield@gmail.com",
		"url": "https://andrewenfield.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/aenfield/blog-eleventy/issues"
	},
	"homepage": "https://github.com/aenfield/blog-eleventy#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.1",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
		"cross-env": "^7.0.3",
		"luxon": "^3.6.1",
		"prismjs": "^1.30.0",
		"zod": "^3.24.4",
		"zod-validation-error": "^3.4.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.3",
		"netlify-plugin-cache": "^1.0.3"
	}
}
